- provide(:title, 'Изменение проекта')
%h1 Изменение проекта
.row
  .span6.offset3.registration-container
    = form_for(@project) do |f|
      = render 'shared/error_messages', object: f.object
      = f.label :name, "Название проекта"
      = f.text_field :name
      = f.label :wiki, "Wiki"
      = f.text_area :wiki
      = f.submit "Сохранить изменения", class: "btn btn-large btn-primary"