- provide(:title, 'Новый проект')
%h1 Новый проект
.row
  .span6.offset3.registration-container
    = form_for(@project) do |f|
      = render 'shared/error_messages', object: f.object
      = f.label :name, "Название проекта"
      = f.text_field :name
      = f.label :wiki, "Wiki"
      = f.text_area :wiki
      = f.label :users, "Сотрудники"
      %form{:action => "form_action.asp"}
        %select{:multiple => "", :name => "cars"}
          %option{:value => "volvo"} Volvo
          %option{:value => "saab"} Saab
          %option{:value => "opel"} Opel
          %option{:value => "audi"} Audi
        %input{:type => "submit"}
      = f.submit "Создать новый проект", class: "btn btn-large btn-primary"